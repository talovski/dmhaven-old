{
  "name": "yoyoyo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && stylelint --fix **/*.{css,tsx} && eslint \"**/*.{tsx,js,ts}\" --fix",
    "postinstall": "husky install",
    "typecheck": "tsc",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky install",
    "scriptname": "cmd",
    "lint:ts:fix": "eslint . --fix **/*.{tsx,js,ts}",
    "lint:css": "stylelint **/*.{tsx,css}",
    "lint:css:fix": "stylelint --fix **/*.{tsx,css}"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@headlessui/react": "^1.6.6",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-popover": "^1.0.0",
    "graphql": "^16.5.0",
    "headlessui": "^0.0.0",
    "next": "12.2.3",
    "next-astroturf": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "^1.0.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@vscode/emmet-helper": "^2.8.4",
    "astroturf": "^1.0.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.2",
    "postcss-sorting": "^7.0.1",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "postcss-utilities": "^0.8.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.7.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && pnpm lint"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "@typescript-eslint/parser",
        "webpack"
      ]
    }
  }
}
